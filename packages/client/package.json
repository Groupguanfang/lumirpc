{
  "name": "@nano-rpc/client",
  "type": "module",
  "version": "0.0.3",
  "description": "Client to connect a nanorpc server.",
  "author": "Naily Zero <zero@naily.cc> (https://naily.cc)",
  "keywords": [
    "es-project-template"
  ],
  "exports": {
    "./axios": {
      "types": "./dist/axios.d.ts",
      "import": "./dist/axios.js",
      "require": "./dist/axios.cjs"
    },
    "./socket": {
      "types": "./dist/socket.d.ts",
      "import": "./dist/socket.js",
      "require": "./dist/socket.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "main": "./dist/axios.cjs",
  "module": "./dist/axios.js",
  "types": "./dist/axios.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup -w",
    "prepublishOnly": "tsup"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nano-rpc/server": "workspace:*",
    "@nano-rpc/types": "workspace:*",
    "axios": "^1.7.9",
    "nanoid": "^5.1.0"
  }
}

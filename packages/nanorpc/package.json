{
  "name": "nanorpc",
  "type": "module",
  "version": "0.0.2",
  "description": "Json rpc protocol base &amp; type safe rpc client and server solutions.",
  "author": "Naily Zero <zero@naily.cc> (https://naily.cc)",
  "keywords": [
    "es-project-template"
  ],
  "exports": {
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "require": "./dist/node.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./axios": {
      "types": "./dist/axios.d.ts",
      "import": "./dist/axios.js",
      "require": "./dist/axios.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./unplugin": {
      "types": "./dist/unplugin.d.ts",
      "import": "./dist/unplugin.js",
      "require": "./dist/unplugin.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.js",
      "require": "./dist/vite.cjs"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.js",
      "require": "./dist/astro.cjs"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.js",
      "require": "./dist/nuxt.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "main": "./dist/node.cjs",
  "module": "./dist/node.js",
  "types": "./dist/node.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup -w",
    "prepublishOnly": "tsup"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@nano-rpc/client": "workspace:*",
    "@nano-rpc/server": "workspace:*",
    "@nano-rpc/types": "workspace:*",
    "@nano-rpc/unplugin": "workspace:*"
  }
}
